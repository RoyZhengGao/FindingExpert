<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ExpertFinding.model.users" table="users">
		<id name="nodeId" column="nodeId"/>
		<property name="id_str" column="id_str"  />
		<property name="name" column="name"  />
		<property name="screen_name" column="screen_name"  />	
		<property name="description" column="description"  />	
		<property name="url" column="url"  />	
		<property name="followers_count" column="followers_count"  />	
		<property name="friends_count" column="friends_count"  />	
		<property name="listed_count" column="listed_count"  />	
		<property name="statuses_count" column="statuses_count"  />	
		<property name="favourites_count" column="favourites_count"  />	
		<property name="location" column="location"  />	
		<property name="time_zone" column="time_zone"  />	
		<property name="utc_offset" column="utc_offset"  />	
		<property name="lang" column="lang"  />
		<property name="profile_image_url" column="profile_image_url"  />
		<property name="geo_enabled" column="geo_enabled"  />
		<property name="verified" column="verified"  />
		<property name="notifications" column="notifications"  />
		<property name="timeline" column="timeline"  />
	</class>
</hibernate-mapping>
